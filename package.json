{
  "name": "betterplace-react-components",
  "version": "1.2.0",
  "description": "betterplace React components",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build",
    "test": "NODE_PATH='./node_modules:src' jest",
    "testc": "NODE_PATH='./node_modules:src' jest --coverage",
    "testw": "NODE_PATH='./node_modules:src' jest --watch"
  },
  "jest": {
    "roots": [
      "src",
      "test"
    ],
    "testMatch": [
      "**/*_test.js?(x)"
    ],
    "setupFiles": [
      "./test/test_helper.js"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betterplace/betterplace-react-components.git"
  },
  "author": "developers@betterplace.org",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/betterplace/betterplace-react-components/issues"
  },
  "homepage": "https://github.com/betterplace/betterplace-react-components",
  "peerDependencies": {
    "i18n-js": "^3.0.3",
    "prop-types": ">=16",
    "react": ">=16"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "i18n-js": "^3.0.3",
    "jest": "^21.3.0-beta.9",
    "jest-enzyme": "^4.0.1",
    "jest-fetch-mock": "^1.3.3",
    "npm": "^5.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^2.1.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {}
}
