{
  "name": "betterplace-react-components",
  "version": "2.0.3",
  "description": "betterplace React components",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && babel src --out-dir dist",
    "prepublish": "npm run build",
    "start": "webpack-dev-server --config webpack.config.js",
    "test": "NODE_PATH='./node_modules:src' NODE_ICU_DATA=./node_modules/full-icu jest",
    "testc": "NODE_PATH='./node_modules:src' NODE_ICU_DATA=./node_modules/full-icu jest --coverage",
    "testw": "NODE_PATH='./node_modules:src' NODE_ICU_DATA=./node_modules/full-icu jest --watch"
  },
  "files": [
    "dist",
    "src"
  ],
  "jest": {
    "roots": [
      "src",
      "test"
    ],
    "testMatch": [
      "**/*_test.js?(x)"
    ],
    "setupFiles": [
      "./test/test_helper.js"
    ],
    "setupFilesAfterEnv": [
      "./node_modules/jest-enzyme/lib/index.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betterplace/betterplace-react-components.git"
  },
  "author": "developers@betterplace.org",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/betterplace/betterplace-react-components/issues"
  },
  "homepage": "https://github.com/betterplace/betterplace-react-components",
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "full-icu": "^1.3.1",
    "html-loader": "^1.3.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.4.2",
    "jest-enzyme": "^7.1.2",
    "jest-fetch-mock": "^3.0.3",
    "mini-css-extract-plugin": "^0.11.2",
    "npm": "^6.14.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "core-js": "^3.6.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
