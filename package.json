{
  "name": "betterplace-react-components",
  "version": "1.8.2",
  "description": "betterplace React components",
  "main": "dist/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build",
    "test": "NODE_PATH='./node_modules:src' NODE_ICU_DATA=./node_modules/full-icu jest",
    "testc": "NODE_PATH='./node_modules:src' NODE_ICU_DATA=./node_modules/full-icu jest --coverage",
    "testw": "NODE_PATH='./node_modules:src' NODE_ICU_DATA=./node_modules/full-icu jest --watch"
  },
  "jest": {
    "roots": [
      "src",
      "test"
    ],
    "testMatch": [
      "**/*_test.js?(x)"
    ],
    "setupFiles": [
      "./test/test_helper.js"
    ],
    "setupFilesAfterEnv": [
      "./node_modules/jest-enzyme/lib/index.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betterplace/betterplace-react-components.git"
  },
  "author": "developers@betterplace.org",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/betterplace/betterplace-react-components/issues"
  },
  "homepage": "https://github.com/betterplace/betterplace-react-components",
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "full-icu": "^1.3.0",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.0.1",
    "jest-fetch-mock": "^2.1.1",
    "npm": "^6.8.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-hot-loader": "^4.7.2",
    "react-scripts": "^3.2.0",
    "webpack": "4.41.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "styled-components": "4.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
